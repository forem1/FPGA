module my_nand(a,b,c);  
input a,b; 					 //输入a由开关KEY1决定，0：按下，1：未按下
                  					  //输入b由开关KEY2决定，0：按下，1：未按下
output c;   							//输出c由LED1显示， 0：点亮，1：熄灭
//assign c=~(a&b); 					 //assign c=~(a|b); 或非门
              									//assign c=a^b; //异或门
              									assign c=a^~b;  // 同或门
endmodule